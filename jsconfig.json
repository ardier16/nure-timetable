{
  "compilerOptions": {
    "target": "esnext",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@store/*": ["./src/store/*"],
      "@common/*": ["./src/common/*"],
      "@router/*": ["./src/router/*"],
      "@utils/*": ["./src/utils/*"],
      "@services/*": ["./src/services/*"],
      "@assets/*": ["./src/assets/*"],
      "@constants/*": ["./src/constants/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
